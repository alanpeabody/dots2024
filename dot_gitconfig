[alias]
  co = checkout
  st = status
  ci = commit -v -S
  cp = cherry-pick
  br = branch
  bra = branch -a
  rmb = !sh -c 'git branch -D $1 && git push origin :$1 --no-verify && git remote prune origin' -
  squash = !sh -c 'git fetch && git rebase -i origin/master -S' -
  pushf = push --force-with-lease
